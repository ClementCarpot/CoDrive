<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}CoDrive{% endblock %}</title>
        <link rel="icon" href="{{ asset('images/home.png') }}">
        {# Run `composer require symfony/webpack-encore-bundle` to start using Symfony UX #}
        {% block stylesheets %}
            {{ encore_entry_link_tags('app') }}
        {% endblock %}

        {% block javascripts %}
            {{ encore_entry_script_tags('app') }}
        {% endblock %}
    </head>
    <body class="pt-15">
        {% include 'header.html.twig' %}

        {% block body %}{% endblock %}
    </body>
</html>
